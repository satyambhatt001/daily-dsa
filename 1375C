#include <bits/stdc++.h>
using namespace std;

#define fast ios::sync_with_stdio(false); cin.tie(nullptr)
#define ln '\n'

int main(){
    fast;

    int t;
    cin >> t;
    while(t--){
        int n;
        cin >> n;
        vector<int> a(n);
        for(int &x : a) cin >> x;
        cout << (a[0] < a[n - 1] ? "YES" : "NO") << ln;
    }
    return 0;
}
